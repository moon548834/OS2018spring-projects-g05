# Sample test file
# The entity name is the same as the file name,
# so there shuld be no file-name suffix

# Line starting with '#' is comment
# The comment should be in seperated lines

# Import a package
# Grammar: "IMPORT <packageName>"
# "IMPORT" should be in UPPER CASE, same below
IMPORT alu_const

# Define a signal to be checked
# Grammar: "DEFINE <signalName> <sub_instace_name.sub_sub_instance_name.(...).signal_name>: <type>"
DEFINE reg regfile_ist.regArray: RegArrayType

# Execute an assembly instruction
# Grammar: "RUN <assembly>"
# 0x00
RUN lui $3, 0xc000
RUN lui $4, 0xc000
RUN mtc1 $3, $2
RUN mtc1 $4, $4
# 0x10
RUN c.ult.d $f2, $f4
RUN bc1t 0x0010
RUN nop
RUN ori $6, $0, 0xcccc
# 0x20
RUN nop
RUN nop
RUN nop
RUN nop
# 0x30
RUN c.ule.d $f2, $f4
RUN bc1t 0x0010
RUN nop
# assert this will never be executed
RUN ori $7, $0, 0xcccc
# 0x40
RUN nop
RUN nop
RUN nop
RUN nop
# 0x50
RUN j 0x50
RUN nop
ASSERT 40 reg(6) x"0000cccc"
ASSERT 40 reg(7) x"00000000"